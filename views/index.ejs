<html>

<head>
    <title>
        <%= title%>
    </title>
</head>

<body>
    <h1>
        <%= title %>
    </h1>
    <% if(webtoons) {%>
        <% webtoons.forEach((webtoon)=> { %>
            <ul>
                <li>ID: <%= webtoon.id%>
                </li>
                <li>Title: <%= webtoon.title%>
                </li>
                <li>Author: <%= webtoon.author%>
                </li>
                <li>Genre: <%= webtoon.genre%>
                </li>
            </ul>

            <%})}%>
                <a href="/create">Create new Webtoon</a>
</body>

</html>